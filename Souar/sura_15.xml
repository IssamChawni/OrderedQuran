<?xml version="1.0" encoding="UTF-8"?><sura><sura index="15" name="الكوثر">
    <aya bismillah="بِسمِ اللَّهِ الرَّحمٰنِ الرَّحيمِ" index="1" text="إِنَّا أَعْطَيْنَاكَ الْكَوْثَرَ"/>
    <aya index="2" text="فَصَلِّ لِرَبِّكَ وَانْحَرْ"/>
    <aya index="3" text="إِنَّ شَانِئَكَ هُوَ الْأَبْتَرُ"/>
  </sura></sura>